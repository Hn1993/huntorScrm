<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              xmlns:tools="http://schemas.android.com/tools"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:orientation="vertical"
              android:background="@color/white"
              tools:context="com.huntor.mscrm.app.ui.fragment.online.InteractionOnlineFragment"
        >

    <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/base_title_height"
            android:background="@color/login_bg"
            >

        <TextView
                android:id="@+id/text_base_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/white"
                android:text="@string/main_interaction_online"
                android:layout_centerInParent="true"
                android:textSize="@dimen/base_title_text_size"
                />

        <ImageView
                android:id="@+id/back"
                android:layout_width="@dimen/base_title_height"
                android:layout_height="match_parent"
                android:textColor="@color/white"
                android:layout_alignParentLeft="true"
                android:src="@drawable/back"
                android:scaleType="centerInside"
                />

        <ImageView
                android:id="@+id/iv_search"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_alignParentEnd="true"
                android:layout_centerVertical="true"
                android:layout_marginEnd="10dp"
                android:src="@drawable/search"/>
    </RelativeLayout>

    <ListView
            android:id="@+id/list_fragment_interaction_online_contact_list"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:divider="@null"
            />


</LinearLayout>
